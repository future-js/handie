{
  "compilerOptions": {
    "strict": true,
    "module": "esnext",
    "moduleResolution": "node",
    "target": "es2015",
    "lib": ["es2015", "es2016", "es2017", "dom"],
    "importHelpers": true,
    "noImplicitThis": false,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "sourceMap": false,
    "noImplicitAny": false,
    "baseUrl": ".",
    "paths": {}
  }
}
